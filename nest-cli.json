{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/admin-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/admin-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/admin-app",
  "projects": {
    "admin-app": {
      "type": "application",
      "root": "apps/admin-app",
      "entryFile": "main",
      "sourceRoot": "apps/admin-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-app/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "library": {
      "type": "library",
      "root": "libs/library",
      "entryFile": "index",
      "sourceRoot": "libs/library/src",
      "compilerOptions": {
        "tsConfigPath": "libs/library/tsconfig.lib.json"
      }
    },
    "order-module": {
      "type": "application",
      "root": "apps/order-module",
      "entryFile": "main",
      "sourceRoot": "apps/order-module/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-module/tsconfig.app.json"
      }
    }
  }
}