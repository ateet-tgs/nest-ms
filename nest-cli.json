{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/admin-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/admin-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/admin-app",
  "projects": {
    "activity-logger": {
      "type": "application",
      "root": "apps/activity-logger",
      "entryFile": "main",
      "sourceRoot": "apps/activity-logger/src",
      "compilerOptions": {
        "tsConfigPath": "apps/activity-logger/tsconfig.app.json"
      }
    },
    "admin-app": {
      "type": "application",
      "root": "apps/admin-app",
      "entryFile": "main",
      "sourceRoot": "apps/admin-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-app/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "mail-service": {
      "type": "application",
      "root": "apps/mail-service",
      "entryFile": "main",
      "sourceRoot": "apps/mail-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mail-service/tsconfig.app.json"
      }
    },
    "order-module": {
      "type": "application",
      "root": "apps/order-module",
      "entryFile": "main",
      "sourceRoot": "apps/order-module/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-module/tsconfig.app.json"
      }
    },
    "rabbitmq": {
      "type": "library",
      "root": "libs/rabbitmq",
      "entryFile": "index",
      "sourceRoot": "libs/rabbitmq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbitmq/tsconfig.lib.json"
      }
    },
    "redis-cache": {
      "type": "library",
      "root": "libs/redis-cache",
      "entryFile": "index",
      "sourceRoot": "libs/redis-cache/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis-cache/tsconfig.lib.json"
      }
    }
  }
}